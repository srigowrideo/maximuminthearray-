NAME Maximum number 
PAGE 5280
TITLE 8086 ALP to find maximum in the array 
.model small
.stack 100 
.data 
ARRAY DB 63H,32H,45H,75H,12H,42H,09H,14H,56H,38H
MAX DB 0
.code 
START:MOV AX,@data 
MOV DS,AX
XOR DI,DI
MOV CL,10
LEA BX,ARRAY
MOV AL,MAX
BACK:CMP AL,[BX+DI]
JNC SKIP 
MOV DL,[BX+DI]
MOV AL,DL
SKIP:INC DI 
DEC CL
JNZ BACK 
MOV MAX,AL
END START 
